<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
</head>
<body class="container">

<header>
    <%- include('../partials/header', {user: username}); %>
</header>

<main>
    <div class="jumbotron">
        <form action="/compose" align="right">
            <button>Compose</button>
        </form>
        <% for (var i=0; i < locals.mail.length; i++) { %>
            <h4><%= locals.mail[i].Subject %></h4>
        <% } %>
    </div>
</main>

</body>
</html>